<%= simple_form_for center do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :email %>
  <%= f.input :location %>
  <%= f.input :language %>
  <%= f.input :photo, as: :file, input_html: { multiple: false } %>
  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
